<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Configure PUBLIC "-//Jetty//Configure//EN" "http://www.eclipse.org/jetty/configure_9_3.dtd">
<Configure id="testWebapp" class="org.eclipse.jetty.webapp.WebAppContext">
  <Set name="contextPath">{{ context_path }}</Set>
  <Set name="war"><Property name="jetty.webapps" default="."/>/cas.war</Set>
  <Set name="extraClasspath"><Property name="jetty.webapps" default="."/>/cas.d</Set>

  <Call name="setInitParameter">
    <Arg>cas.properties.config.location</Arg>
    <Arg>file:<Property name="jetty.webapps" default="."/>/cas.d/cas.properties</Arg>
  </Call>
</Configure>
